<?xml version="1.0" encoding="UTF-8"?>headers

<plugin xmlns="http://www.w3.org/ns/widgets"
    id="nat-communication"
    version="0.1.0">
    <name>nat-communication</name>
    <description>nat communication</description>
    <license>MIT</license>
    <keywords>weex,plugin,nat,natjs,</keywords>
    <repo>https://github.com/Natjs/weex-nat-communication.git</repo>
    <issue>https://github.com/Natjs/weex-nat-communication/issues</issue>
    
     <!-- ios -->
     <platform name="ios">
         <framework src="UIKit.framework" />
          <framework src="NatCommunication" type="podspec" spec="" />
         <config-file target="WeexpluginConfig.xml" parent="/*">
             <feature name="WXTestModule">
                 <param name="category" value="module" />
                 <param name="ios-package" value="WeexNatComm" />
                 <param name="api" value="nat/communication" />
             </feature>
         </config-file>
         <header-file src="ios/WeexNatComm.h"/>
         <source-file src="ios/WeexNatComm.m"/>
     </platform>

     <!-- android -->
     <platform name="android">
     </platform>

</plugin>
